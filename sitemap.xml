---
layout: none
nositemap: "yes"
---

<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for post in site.posts %}<url><loc>{{ post.url | absolute_url }}</loc><lastmod>{{ post.last_modified_at | default: post.date | date: "%Y-%m-%d" }}</lastmod></url>
    {% endfor %}
    {% for page in site.pages %}{% unless page.nositemap == "yes" or page.url contains '/css/' or page.url contains '404' %}<url><loc>{{ page.url | absolute_url }}</loc>{% if page.layout == 'page' %}{% unless page.nolastmod == 'yes' %}<lastmod>{{ page.last_modified_at | date: "%Y-%m-%d" }}</lastmod>{% endunless %}{% endif %}</url>
    {% endunless %}{% endfor %}
</urlset>
